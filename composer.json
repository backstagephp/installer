{
    "name": "backstage/installer",
    "version": "0.0.1",
    "description": "Backstage installer.",
    "keywords": [
        "laravel", "cms", "backstage"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu de Ruiter",
            "email": "mathieu@vormkracht10.nl"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/filesystem": "^10.20|^11.0|^12.0",
        "illuminate/support": "^10.20|^11.0|^12.0",
        "laravel/prompts": "^0.1.18|^0.2.0|^0.3.0",
        "symfony/console": "^6.2|^7.0",
        "symfony/process": "^6.2|^7.0",
        "symfony/polyfill-mbstring": "^1.31"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.4"
    },
    "bin": [
        "bin/backstage"
    ],
    "autoload": {
        "psr-4": {
            "Backstage\\Installer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backstage\\Installer\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}